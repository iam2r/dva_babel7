{
  "name": "webpack4-learn",
  "version": "0.0.1",
  "description": "learn webpack4 config change",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV='production' webpack --config build/webpack.prod.config.js --mode production --report",
    "dev": "cross-env NODE_ENV='development' webpack-dev-server --config build/webpack.dev.config.js --mode development --hot",
    "dll": "webpack --config build/webpack.dll.config.js --mode development",
    "start": "npm run dll && npm run dev",
    "serve": "http-server ./dist -o"
  },
  "keywords": [
    "webpack4",
    "cli"
  ],
  "author": "zr",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^8.6.2",
    "awesome-typescript-loader": "^5.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.8.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-hot-loader": "^1.3.9",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "happypack": "^5.0.0-beta.3",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.8.3",
    "open-browser-webpack-plugin": "0.0.5",
    "optimize-css-assets-webpack-plugin": "^4.0.2",
    "postcss-loader": "^2.1.4",
    "postcss-px2rem-exclude": "0.0.6",
    "progress-bar-webpack-plugin": "^1.11.0",
    "sass-loader": "^7.0.3",
    "typescript": "^2.9.2",
    "url-loader": "^1.0.1",
