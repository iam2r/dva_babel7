    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2",
    "webpack-parallel-uglify-plugin": "^1.1.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "antd": "^3.10.6",
    "axios": "^0.18.0",
    "dva": "^2.4.1",
    "react": "^16.6.1",
    "react-dom": "^16.6.1"
  }
}
